@model IEnumerable<FootballShare.Entities.Group.GroupPool>

@{
    ViewData["Title"] = "View";
}

<h1>My Betting Pools</h1>

<p>
    <a asp-action="Create">Create New Pool</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SeasonId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SeasonId)
            </td>
        </tr>
}
    </tbody>
</table>
